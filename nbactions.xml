<?xml version="1.0" encoding="UTF-8"?>
<actions>
	<action>
		<actionName>build</actionName>
		<packagings>
			<packaging>*</packaging>
		</packagings>
		<goals>
			<goal>package</goal>
		</goals>
	</action>
	<action>
		<actionName>rebuild</actionName>
		<packagings>
			<packaging>*</packaging>
		</packagings>
		<goals>
			<goal>clean</goal>
			<goal>package</goal>
		</goals>
	</action>
	<action>
		<actionName>build-with-dependencies</actionName>
		<reactor>also-make</reactor>
		<packagings>
			<packaging>*</packaging>
		</packagings>
		<goals>
			<goal>package</goal>
		</goals>
	</action>
	<action>
		<actionName>clean</actionName>
		<packagings>
			<packaging>*</packaging>
		</packagings>
		<goals>
			<goal>clean</goal>
			<goal>generate-sources</goal>
		</goals>
	</action>
	<action>
		<actionName>run</actionName>
		<packagings>
			<packaging>*</packaging>
		</packagings>
		<goals>
			<goal>process-classes</goal>
			<goal>spring-boot:run</goal>
		</goals>
		<properties>
			<server.port>8080</server.port>
			<server.context-path></server.context-path>
		</properties>
	</action>
	<action>
		<actionName>debug</actionName>
		<packagings>
			<packaging>*</packaging>
		</packagings>
		<goals>
			<goal>process-classes</goal>
			<goal>spring-boot:run</goal>
		</goals>
		<properties>
			<debug></debug>
			<server.port>8080</server.port>
			<server.context-path></server.context-path>
			<jpda.listen>maven</jpda.listen>
		</properties>
	</action>
	<action>
		<actionName>CUSTOM-sortpom:sort</actionName>
		<displayName>sortpom:sort</displayName>
		<goals>
			<goal>sortpom:sort</goal>
		</goals>
	</action>
	<action>
		<actionName>test.single</actionName>
		<packagings>
			<packaging>*</packaging>
		</packagings>
		<goals>
			<goal>test</goal>
		</goals>
		<properties>
			<test>${packageClassName}</test>
		</properties>
	</action>
</actions>
